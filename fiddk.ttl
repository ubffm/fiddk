#################################################################
# foaf:Person
#################################################################

fiddk-sh:PersonShape
    a sh:NodeShape ;
    sh:targetClass foaf:Person ;



    # 1) max. ein prefLabel pro *nicht-leerem* Sprach-Tag
    sh:property [
        sh:path skos:prefLabel ;
        sh:nodeKind sh:Literal ;
        sh:uniqueLang true
    ] ;

    # 2) max. ein prefLabel ohne Sprach-Tag
    sh:sparql [
        a sh:SPARQLConstraint ;
        sh:message "Maximal ein skos:prefLabel ohne Sprach-Tag pro Ressource."@de ;
        sh:select """
          PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

          SELECT $this
          WHERE {
            $this skos:prefLabel ?l .
            FILTER ( isLiteral(?l) && LANG(?l) = "" )
          }
          GROUP BY $this
          HAVING (COUNT(?l) > 1)
        """
    ] ;

    sh:property [
        sh:path skos:altLabel ;
        sh:minCount 0 ;
        sh:nodeKind sh:Literal ;
    ] ;

    sh:property [
        sh:path skos:note ;
        sh:minCount 0 ;
        sh:nodeKind sh:Literal ;
    ] ;

    sh:property [
        sh:path dc:date ;
        sh:minCount 0 ;
    ] ;

    sh:property [
        sh:path dc:identifier ;
        sh:minCount 0 ;
        sh:nodeKind sh:Literal ;
    ] ;

    sh:property [
        sh:path edm:hasMet ;
        sh:minCount 0 ;
    ] ;

    sh:property [
        sh:path edm:isRelatedTo ;
        sh:minCount 0 ;
    ] ;

    sh:property [
        sh:path edm:wasPresentAt ;
        sh:minCount 0 ;
        sh:nodeKind sh:IRI ;
    ] ;

    sh:property [
        sh:path rdau:P60492 ;  # biographicalInformation
        sh:minCount 0 ;
        sh:nodeKind sh:Literal ;
    ] ;

    sh:property [
        sh:path rdau:P60599 ;  # dateOfBirth
        sh:minCount 0 ;
        sh:maxCount 1 ;
    ] ;

    sh:property [
        sh:path rdau:P60598 ;  # dateOfDeath
        sh:minCount 0 ;
        sh:maxCount 1 ;
    ] ;

    sh:property [
        sh:path rdau:P60594 ;  # placeOfBirth
        sh:minCount 0 ;
        sh:maxCount 1 ;
    ] ;

    sh:property [
        sh:path rdau:P60592 ;  # placeOfDeath
