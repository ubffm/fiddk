#################################################################
# foaf:Person
#################################################################

fiddk-sh:PersonShape
    a sh:NodeShape ;
    sh:targetClass foaf:Person ;

    sh:property [
        sh:path skos:prefLabel ;
        sh:minCount 0 ;
        sh:maxCount 1 ;
        sh:nodeKind sh:Literal ;
    ] ;

    sh:property [
        sh:path skos:altLabel ;
        sh:minCount 0 ;
        sh:nodeKind sh:Literal ;
    ] ;

    sh:property [
        sh:path skos:note ;
        sh:minCount 0 ;
        sh:nodeKind sh:Literal ;
