#################################################################
# edm:ProvidedCHO
#################################################################

fiddk-sh:ProvidedCHOShape
    a sh:NodeShape ;
    sh:targetClass edm:ProvidedCHO ;

    # dc:contributor (0..*)
    sh:property [
        sh:path dc:contributor ;
        sh:minCount 0 ;
    ] ;

    # dc:coverage (0..*), einer von dc:subject, dc:type, dcterms:spatial oder dcterms:temporal ist Pflicht
    sh:property [
        sh:path dc:coverage ;
        sh:minCount 0 ;
    ] ;

    # dc:creator (0..*)
    sh:property [
        sh:path dc:creator ;
        sh:minCount 0 ;
    ] ;

    # dc:description (0..*) â€“ Beschreibung ODER Titel muss vorhanden sein
    sh:property [
        sh:path dc:description ;
        sh:minCount 0 ;
    ] ;

    # dc:identifier (0..*)
    sh:property [
        sh:path dc:identifier ;
        sh:minCount 0 ;
        sh:datatype xsd:string ;
    ] ;

    # dc:language (0..*)
    sh:property [
        sh:path dc:language ;
        sh:minCount 0 ;
        sh:nodeKind sh:Literal ;
    ] ;

    # dc:publisher (0..*)
    sh:property [
        sh:path dc:publisher ;
        sh:minCount 0 ;
    ] ;

    # dc:relation (0..*)
    sh:property [
        sh:path dc:relation ;
        sh:minCount 0 ;
    ] ;

    # dc:rights (0..*)
    sh:property [
        sh:path dc:rights ;
        sh:minCount 0 ;
    ] ;

    # dc:source (0..*)
    sh:property [
        sh:path dc:source ;
        sh:minCount 0 ;
    ] ;

    # dc:subject (0..*)
    sh:property [
        sh:path dc:subject ;
        sh:minCount 0 ;
    ] ;

    # dc:title (0..1)
    sh:property [
        sh:path dc:title ;
        sh:minCount 0 ;
        sh:maxCount 1 ;
        sh:nodeKind sh:Literal ;
    ] ;

    # dc:type (1..*)
    sh:property [
        sh:path dc:type ;
        sh:minCount 1 ;
    ] ;

    # dcterms:alternative (0..*)
    sh:property [
        sh:path dcterms:alternative ;
        sh:minCount 0 ;
    ] ;

    # dcterms:created (0..*)
    sh:property [
        sh:path dcterms:created ;
        sh:minCount 0 ;
    ] ;

    # dcterms:extent (0..*)
    sh:property [
        sh:path dcterms:extent ;
        sh:minCount 0 ;
    ] ;

    # dcterms:hasPart (0..*)
    sh:property [
        sh:path dcterms:hasPart ;
        sh:minCount 0 ;
    ] ;

    # dcterms:isPartOf (0..*)
    sh:property [
        sh:path dcterms:isPartOf ;
        sh:minCount 0 ;
    ] ;

    # dcterms:issued (0..*)
    sh:property [
        sh:path dcterms:issued ;
        sh:minCount 0 ;
    ] ;

    # dcterms:medium (0..*)
    sh:property [
        sh:path dcterms:medium ;
        sh:minCount 0 ;
    ] ;

    # dcterms:provenance (0..*)
    sh:property [
        sh:path dcterms:provenance ;
        sh:minCount 0 ;
    ] ;

    # dcterms:references (0..*)
    sh:property [
        sh:path dcterms:references ;
        sh:minCount 0 ;
    ] ;

    # dcterms:spatial (0..*)
    sh:property [
        sh:path dcterms:spatial ;
        sh:minCount 0 ;
    ] ;

    # dcterms:tableOfContents (0..*)
    sh:property [
        sh:path dcterms:tableOfContents ;
        sh:minCount 0 ;
    ] ;

    # dcterms:temporal (0..*)
    sh:property [
        sh:path dcterms:temporal ;
        sh:minCount 0 ;
    ] ;

    # edm:currentLocation (1..*)
    sh:property [
        sh:path edm:currentLocation ;
        sh:minCount 1 ;
    ] ;

    # edm:type (0..1)
    sh:property [
        sh:path edm:type ;
        sh:minCount 0 ;
        sh:maxCount 1 ;
        sh:nodeKind sh:Literal ;
    ] ;

    # owl:sameAs (0..*)
    sh:property [
        sh:path <http://www.w3.org/2002/07/owl#sameAs> ;
        sh:minCount 0 ;
        sh:nodeKind sh:IRI ;
    ] .

